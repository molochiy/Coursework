@{
  ViewBag.Title = "Coursework";
}
<div class="page-content">
  <div class="left-side">
    <div class="user-info">
      <ul>
        @foreach (var state in ViewBag.States)
        {
          <li>@state.Name</li>
        }
      </ul>
    </div>
    <div class="input-menu">
      <ul>
        @foreach (var role in ViewBag.Roles)
        {
          <li>@role.Name</li>
        }
      </ul>
    </div>
  </div>
  <div class="right-side">
    <div class="plot"></div>
    <div class="plot"></div>
  </div>
</div>